# Sample Data for Social TypeDB Network
# This file contains TypeQL insert queries to populate the database with test data

# Insert Persons
insert
$alice isa person, has username "alice_smith", has email "alice@example.com", has full-name "Alice Smith";
$bob isa person, has username "bob_jones", has email "bob@example.com", has full-name "Bob Jones";
$charlie isa person, has username "charlie_brown", has email "charlie@example.com", has full-name "Charlie Brown";
$diana isa person, has username "diana_prince", has email "diana@example.com", has full-name "Diana Prince";
$eve isa person, has username "eve_adams", has email "eve@example.com", has full-name "Eve Adams";
$frank isa person, has username "frank_miller", has email "frank@example.com", has full-name "Frank Miller";
$grace isa person, has username "grace_hopper", has email "grace@example.com", has full-name "Grace Hopper";
$henry isa person, has username "henry_ford", has email "henry@example.com", has full-name "Henry Ford";
$iris isa person, has username "iris_watson", has email "iris@example.com", has full-name "Iris Watson";
$jack isa person, has username "jack_sparrow", has email "jack@example.com", has full-name "Jack Sparrow";

# Insert Organizations
insert
$techcorp isa organization, has name "TechCorp Inc", has organization-type "company";
$university isa organization, has name "State University", has organization-type "educational-institute";
$charity isa organization, has name "Help Foundation", has organization-type "charity";
$startup isa organization, has name "InnovateLab", has organization-type "company";
$hospital isa organization, has name "City Hospital", has organization-type "company";

# Insert Groups
insert
$developers isa group, has name "Developers United", has description "A group for software developers";
$photographers isa group, has name "Photo Enthusiasts", has description "Photography lovers community";
$runners isa group, has name "Morning Runners", has description "Early morning running group";
$bookclub isa group, has name "Book Club", has description "Monthly book reading discussions";
$gamers isa group, has name "Gaming Squad", has description "Video game enthusiasts";

# Insert Posts
insert
$post1 isa post, has title "Welcome to TypeDB!", has content "Just started learning TypeDB and it's amazing!", has created-at 2024-01-15T10:30:00;
$post2 isa post, has title "Morning Run Complete", has content "Just finished a 5K run in the park. Beautiful weather today!", has created-at 2024-01-15T08:15:00;
$post3 isa post, has title "New Project Launch", has content "Excited to announce our new AI project at TechCorp!", has created-at 2024-01-14T16:45:00;
$post4 isa post, has title "Book Recommendation", has content "Just finished reading 'The Pragmatic Programmer'. Highly recommend!", has created-at 2024-01-14T20:30:00;
$post5 isa post, has title "Photography Tips", has content "Golden hour is the best time for outdoor portraits. Here are my top 5 tips...", has created-at 2024-01-13T18:20:00;
$post6 isa post, has title "University Lecture", has content "Gave a guest lecture on database systems today. Students were very engaged!", has created-at 2024-01-13T14:00:00;
$post7 isa post, has title "Charity Event Success", has content "Our fundraising event raised $50,000 for local families. Thank you all!", has created-at 2024-01-12T19:15:00;
$post8 isa post, has title "Gaming Tournament", has content "Won first place in the local esports tournament! Team work makes the dream work.", has created-at 2024-01-12T22:45:00;
$post9 isa post, has title "Startup Life", has content "Working 16 hour days but loving every minute of building something new.", has created-at 2024-01-11T23:30:00;
$post10 isa post, has title "Medical Breakthrough", has content "Our research team made a significant discovery in cancer treatment.", has created-at 2024-01-11T12:00:00;

